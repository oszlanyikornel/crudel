{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"strict": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"rootDir": ".",
		"outDir": "./dist",
		"esModuleInterop": true
	},
	"include": ["**/*.ts", "*.ts"],
	"exclude": ["node_modules"]
}
